{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "ext-dom": "*",
    "flekssk/laravel-util": "v2.1.1",
    "guzzlehttp/guzzle": "^7.9|^8.0",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/socialite": "^5.12",
    "laravel/tinker": "^2.9",
    "phpstan/phpdoc-parser": "^2.2",
    "predis/predis": "^2.2",
    "socialiteproviders/vkontakte": "^5.0",
    "torann/geoip": "^3.0",
    "yajra/laravel-datatables-oracle": "^12.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.1 || ^9.0",
    "phpunit/phpunit": "^11.0",
    "kitloong/laravel-migrations-generator": "^7.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/seeders",
      "database/factories"
    ],
    "files": [
      "app/helpers.php"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/flekssk/laravel-util.git"
    }
  ]
}
